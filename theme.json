							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/lora_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/lora_italic_600.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lora_italic_700.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid":false,
					"size": "0.75em",
					"name": "2",
					"slug": "2"
				},
				{
					"fluid":false,
					"size": "1em",
					"name": "3",
					"slug": "4"
				},
				{
					"fluid":true,
					"size": "1.25em",
					"name": "7",
					"slug": "7"
				},
				{
					"fluid":true,
					"size": "1.5em",
					"name": "8",
					"slug": "8"
				},
				{
					"fluid":true,
					"size": "1.875em",
					"name": "9",
					"slug": "9"
				}
			]
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--normal)"
		},
		"spacing": {
			"blockGap": "1em",
			"padding": {
				"left": "clamp(0.5em, 9vw, 5.93em)",
				"right": "clamp(0.5em, 9vw, 5.93em)",
				"top": "124px",
				"bottom": "2em"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				},
				"border": {
					"bottom": {
						"color":"var(--wp--preset--color--gray)",
						"style": "solid",
						"width": "0.05em"
					}
				},
				":hover": {
					"css": "opacity: 1"
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600"
				}	
			}
		},
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						"border": {
							"style": "none"
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"border": {
							"style": "none"
						}
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}