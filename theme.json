{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"position": {
			"sticky": true
		},
		"layout": {
			"contentSize": "705px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "1"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "2"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "3"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "4"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "5"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "6"
				}
			]
		},
		"color": {
			"custom": true,
			"defaultPalette": false,
			"palette": [
				{
					"color": "rgb(255,255,255)",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "rgb(55, 53, 47)",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "rgb(255,255,254)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#787774",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#9F6B53",
					"name": "Brown",
					"slug": "brown"
				},
				{
					"color": "#D9730D",
					"name": "Orange",
					"slug": "orange"
				},
				{
					"color": "#CB912F",
					"name": "Yellow",
					"slug": "yellow"
				},
				{
					"color": "#448361",
					"name": "Green",
					"slug": "green"
				},
				{
					"color": "#337EA9",
					"name": "Blue",
					"slug": "blue"
				},
				{
					"color": "#9065B0",
					"name": "Purple",
					"slug": "purple"
				},
				{
					"color": "#C14C8A",
					"name": "Pink",
					"slug": "pink"
				},
				{
					"color": "#D44C47",
					"name": "Red",
					"slug": "red"
				},
				{
					"color": "rgba(255,255,255,0.9)",
					"name": "Tint",
					"slug": "tint"
				}
			]
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Normal",
					"slug": "normal"
				},
				{
					"fontFamily": "Lora",
					"slug": "lora",
					"fontFace": [
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lora_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/lora_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/lora_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lora_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "400",
