{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"position": {
			"sticky": true
		},
		"layout": {
			"contentSize": "705px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"size": "clamp(0.5rem, 5vw, 1rem)",
					"slug": "20",
					"name": "2"
				},
				{
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30",
					"name": "3"
				},
				{
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40",
					"name": "4"
				},
				{
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50",
					"name": "5"
				},
				{
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60",
					"name": "6"
				},
				{
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70",
					"name": "7"
				},
				{
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80",
					"name": "8"
				}
			]
		},
		"color": {
			"custom": true,
			"defaultPalette": false,
			"palette": [
				{
					"color": "rgb(255,255,255)",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "rgb(55, 53, 47)",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "rgb(35, 131, 226)",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#787774",
					"name": "Gray",
					"slug": "gray"
				},
				{
					"color": "#9F6B53",
					"name": "Brown",
					"slug": "brown"
				},
				{
					"color": "#D9730D",
					"name": "Orange",
					"slug": "orange"
				},
				{
					"color": "#CB912F",
					"name": "Yellow",
					"slug": "yellow"
				},
				{
					"color": "#448361",
					"name": "Green",
					"slug": "green"
				},
				{
					"color": "#337EA9",
					"name": "Blue",
					"slug": "blue"
				},
				{
					"color": "#9065B0",
					"name": "Purple",
					"slug": "purple"
				},
				{
					"color": "#C14C8A",
					"name": "Pink",
					"slug": "pink"
				},
				{
					"color": "#D44C47",
					"name": "Red",
					"slug": "red"
				},
				{
					"color": "rgba(255,255,255,0.9)",
					"name": "Tint",
					"slug": "tint"
				}
			]
		},
		"typography": {
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "ui-sans-serif, -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "Normal",
					"slug": "normal"
				},
				{
					"fontFamily": "Lora",
					"slug": "lora",
					"fontFace": [
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lora_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/lora_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/lora_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lora_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/lora_italic_400.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/lora_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/lora_italic_600.ttf"
							]
						},
						{
							"fontFamily": "Lora",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lora_italic_700.ttf"
							]
						}
					]
				}
			],
			"fontSizes": [
				{
					"fluid":false,
					"size": "0.75em",
					"name": "2",
					"slug": "2"
				},
				{
					"fluid":false,
					"size": "0.875em",
					"name": "3",
					"slug": "3"
				},
				{
					"fluid":false,
					"size": "1em",
					"name": "4",
					"slug": "4"
				},
				{
					"fluid":true,
					"size": "1.25em",
					"name": "7",
					"slug": "7"
				},
				{
					"fluid":true,
					"size": "1.5em",
					"name": "8",
					"slug": "8"
				},
				{
					"fluid":true,
					"size": "1.875em",
					"name": "9",
					"slug": "9"
				}
			]
		},
		"custom": {
			"styleOpacity": "0.65"
		}
	},
	"styles": {
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--normal)",
			"lineHeight": "1.5",
			"fontSize": "var(--wp--preset--font-size--4)"
		},
		"spacing": {
			"blockGap": "1em",
			"padding": {
				"left": "clamp(0.5em, 9vw, 5.93em)",
				"right": "clamp(0.5em, 9vw, 5.93em)",
				"top": "0",
				"bottom": "2em"
			}
		},
		"elements": {
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				},
				"border": {
					"bottom": {
						"color":"var(--wp--preset--color--gray)",
						"style": "solid",
						"width": "0.05em"
					}
				},
				":hover": {
					"css": "opacity: 1"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2)",
					"lineHeight": "1.4"
				}
			},
			"button": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--2)",
					"lineHeight": "1.1",
					"fontWeight": "500"
				},
				"color": {
					"text": "var(--wp--preset--color--white)",
					"background": "var(--wp--preset--color--primary)"
				},
				"border": {
					"radius": "0.1875em",
					"width": "1px",
					"color": "rgba(55, 53, 47, 0.16)",
					"style": "solid"
				},
				"spacing": {
					"padding": {
						"left": "0.5em",
						"right": "0.5em",
						"top": "0.5em",
						"bottom": "0.5em"
					}
				}
			},
			"heading": {
				"typography": {
					"fontWeight": "600"
				}	
			},
			"h1": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--9)"
				}	
			},
			"h2": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--8)"
				}	
			},
			"h3": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--7)"
				}	
			},
			"h4": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--6)"
				}	
			},
			"h5": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--5)"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--4)"
				}	
			}
		},
		"blocks": {
			"core/site-title": {
				"elements": {
					"link": {
						"border": {
							"style": "none"
						}
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"border": {
							"style": "none"
						}
					}
				}
			},
			"core/button": {
				"variations": {
					"outline": {
						"border": {
							"radius": "3px",
							"width": "0"
						},
						"spacing": {
							"padding": {
								"left": "calc(0.5em + 1px)",
								"right": "calc(0.5em + 1px)",
								"top": "calc(0.5em + 1px)",
								"bottom": "calc(0.5em + 1px)"
							}
						}
					}
				}
			},
			"core/separator": {
				"border": {
					"bottom": {
						"style": "none"
					}
				}
			},
			"core/quote": {
				"border": {
					"left": {
						"style": "solid",
						"color": "var(--wp--preset--color--contrast)",
						"width": "0.1875em"
					}
				},
				"spacing": {
					"padding": {
						"left": "0.875em",
						"right": "0.875em"
					}
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"customTemplates": [
		{
			"name": "serif",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Serif"
		},
		{
			"name": "mono",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Mono"
		}
	],
	"version": 2
}